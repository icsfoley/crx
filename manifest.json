{
  "manifest_version": 3,
  "name": "Sbeve",
  "description": "Does something",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["sbeve.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Default title from JSON"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab"
  ]
}
